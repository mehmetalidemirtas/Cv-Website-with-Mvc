
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="~/Admin_Thema/css/css.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin Panel</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="/Admin_Thema/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="/Admin_Thema/dist/css/adminlte.min.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body>

    <div class="topnav" id="myTopnav">
        <a href="/Default/Index/">Go to Website</a>
        <a href="/About/Index/">About</a>
        <a href="/Experiences/Index/">Experience</a>
        <a href="/Education/Index/">Education</a>
        <a href="/Skills/Index/">Skills</a>
        <a href="/Interests/Index/">Interests</a>
        <a href="/Certification/Index/">Certifications</a>
        <a href="/Contact/Index/">Contact</a>
        <a href="/SocialMedia/Index/">Social Media</a>
        <a href="/ProgrammingLanguages/Index/">Programming Languages</a>
        <a href="/Admin/Index/">Admin</a>
        <a href="/Login/Logout/">Exit</a>
        <a href="javascript:void(0);" style="font-size:15px;" class="icon" onclick="myFunction()">&#9776;</a>
    </div>

    <div class="container">
        @RenderBody()
    </div>
    <script>
        function myFunction() {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }
    </script>
    <!-- jQuery -->
    <script src="/Admin_Thema/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/Admin_Thema/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Admin_Thema/dist/js/adminlte.min.js"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="/Admin_Thema/dist/js/demo.js"></script>
</body>
</html>

