@model MvcCvProject.Models.Entity.Table_ProgrammingLanguages
@{
    ViewBag.Title = "UpdatePL";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="container">
    <div class="col-12">
        <br />
        <h2>Update Programming Language</h2>
        <br />
        <form method="post">
            @Html.Label("Name")
            @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
            <br />
            @Html.Label("Link")
            @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
            <br />            
            <button class="btn btn-success">Update</button>
        </form>
    </div>
    <div class="col">
    </div>
</div>
